{
  "name": "react-native-twilio-phone",
  "version": "1.6.0",
  "description": "Twilio Voice React Native module.",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "android",
    "ios",
    "cpp",
    "react-native-twilio-phone.podspec",
    "!lib/typescript/example"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/MrHertal/react-native-twilio-phone",
  "author": "Gr√©goire Hertault (https://github.com/MrHertal)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrHertal/react-native-twilio-phone/issues"
  },
  "homepage": "https://github.com/MrHertal/react-native-twilio-phone#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-firebase/app": "^11.5.0",
    "@react-native-firebase/messaging": "^11.5.0",
    "@release-it/conventional-changelog": "^2.0.1",
    "@types/jest": "^26.0.0",
    "@types/react": "17.0.1",
    "@types/react-native": "0.64.1",
    "commitlint": "^12.1.4",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^6.0.0",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-builder-bob": "^0.18.1",
    "react-native-callkeep": "^4.0.1",
    "react-native-voip-push-notification": "^3.1.0",
    "release-it": "^14.6.2",
    "typescript": "~4.0.0"
  },
  "peerDependencies": {
    "@react-native-firebase/app": "*",
    "@react-native-firebase/messaging": "*",
    "react": "*",
    "react-native": "*",
    "react-native-callkeep": "*",
    "react-native-voip-push-notification": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "Release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "dependencies": {
    "uuid-random": "^1.3.2"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
